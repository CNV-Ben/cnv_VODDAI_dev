    <sql>update SCTE_REQUEST t
  set t.identity_vod_endpt_id = (select id from dai_reporting.vod_endpoint where identity = t.identity)
  where t.identity_vod_endpt_id is null;

update SCTE_REQUEST t
  set t.adm_data_vod_endpt_id = (select id from dai_reporting.vod_endpoint where identity = t.adm_data)
  where T.ADM_DATA_VOD_ENDPT_ID is null;


commit;
