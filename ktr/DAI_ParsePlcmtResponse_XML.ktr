        <class_source><![CDATA[// Placement Response grouping
//-------------------------------------
//
// field positions (input)
private int posXMLDataTypeDescription;
private int posXMLElementLevel;
private int posXMLPath;
private int posXMLDataName;
private int posXMLDataValue;
//-------------------------------------
// field positions (output)
private int posOutputGroup;
private int posOutputXMLKey;
private int posOutputRespMsgId;
private int posOutputplcmtGroup;
private int posOutputSessionId;
private int posOutputDecisionId;
private int posOutputOppoId;
private int groupVal=1;
//---------------------------------------
// grouping
private String respMsgIdVal="";
private String sessionIdVal="";
private String decisionIdVal="";
private String oppoIdVal="";
private long plcmtGroupVal=1;

private static final int GROUP_LEVEL_CHECK=2;
private static final int PLCMT_GROUP_LEVEL_CHECK=4; // Level 3 is for PlacementDecision
//-----------------------------------------------------
private static final String REGEX_PATTERN_XML_PATH = "(/)(.+?)(:)(\\w+)";     // xml_path pattern
private static final String REGEX_PATTERN_XML_DATANAME = "(^)(.+?)(:)(\\w+)"; // xml_data_name pattern
private static final String REGEX_OUTPUT_PATTERN = "$1$4"; 
private static final String REGEX_PATTERN_XMLKEY = "(/)(.+?)(/)(.+?)(/)(\\w+)";
private static final String REGEX_OUTPUT_XMLKEY = "$5$6"; 
  
public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException {
  Object[] r=getRow();

  if (r==null) {
     setOutputDone();
	 return false;
  }

  if (first) {
	 // get field positions (input)
     posXMLDataTypeDescription = getInputRowMeta().indexOfValue("xml_data_type_description");
     posXMLElementLevel = getInputRowMeta().indexOfValue("xml_element_level");  
     posXMLPath = getInputRowMeta().indexOfValue("xml_path");  
     posXMLDataName = getInputRowMeta().indexOfValue("xml_data_name"); 
     posXMLDataValue = getInputRowMeta().indexOfValue("xml_data_value"); 
    
	 // get field positions (output)
     posOutputGroup = data.outputRowMeta.indexOfValue("group");
     posOutputXMLKey = data.outputRowMeta.indexOfValue("xmlKey");
     posOutputRespMsgId = data.outputRowMeta.indexOfValue("RespMsgId");
     posOutputplcmtGroup = data.outputRowMeta.indexOfValue("plcmtGroup");
     posOutputSessionId = data.outputRowMeta.indexOfValue("sessionId");
     posOutputDecisionId = data.outputRowMeta.indexOfValue("decisionId");
     posOutputOppoId = data.outputRowMeta.indexOfValue("oppoId");

     if (posXMLDataTypeDescription<0 || posXMLElementLevel<0 || posOutputGroup<0)  {
         throw new KettleException("One of the xml_ fields (xml_data_type_description, xml_element_level, or output fields was not found.");
     }
	 
     first=false;
  }

  // set output value: groupVal
  if ( ((Long)r[posXMLElementLevel]==GROUP_LEVEL_CHECK) && (((String)r[posXMLDataTypeDescription]).equalsIgnoreCase("START_ELEMENT")) ){
    groupVal=groupVal*(-1);
  }

  // set output value: oppoGroupVal
  if ( ((Long)r[posXMLElementLevel]==PLCMT_GROUP_LEVEL_CHECK) && 
       (((String)r[posXMLDataTypeDescription]).equalsIgnoreCase("START_ELEMENT")) && 
        ((String) r[posXMLPath]).toUpperCase().endsWith("PLACEMENT") ){
    plcmtGroupVal=plcmtGroupVal+1;
  }

  // set output value: xmlKey
  StringBuffer sb = new StringBuffer("");
  if ( ((Long)r[posXMLElementLevel] >= GROUP_LEVEL_CHECK) ) {
    String xmlPathStr = ((String) r[posXMLPath]).toUpperCase();
    String xmlDNStr = ((String) r[posXMLDataName]).toUpperCase();
    String tmpStr1 = xmlPathStr.replaceAll(REGEX_PATTERN_XML_PATH, REGEX_OUTPUT_PATTERN);
    //String tmpStr2 = tmpStr1.replaceAll(REGEX_PATTERN_XMLKEY, REGEX_OUTPUT_XMLKEY);
    String tmpStr3 = xmlDNStr.replaceAll(REGEX_PATTERN_XML_DATANAME, REGEX_OUTPUT_PATTERN);
    sb.append(tmpStr1);
    sb.append("/");
    sb.append(tmpStr3);
  }

  if (sb.toString().equals("/PLACEMENTRESPONSE/MESSAGEID")) {
     respMsgIdVal = (String) r[posXMLDataValue];
   }

  if (sb.toString() != null && 
      sb.toString().equals("/PLACEMENTRESPONSE/SYSTEMCONTEXT/SESSION/SESSION") && 
     (!((String)r[posXMLDataTypeDescription]).equalsIgnoreCase("START_ELEMENT")) ) {
     sessionIdVal = (String) r[posXMLDataValue];
  }

  if (sb.toString() != null &&
      sb.toString().equals("/PLACEMENTRESPONSE/PLACEMENTDECISION/ID") &&
      (!((String)r[posXMLDataTypeDescription]).equalsIgnoreCase("START_ELEMENT")) ) {
     decisionIdVal = (String) r[posXMLDataValue];
  }

  if (sb.toString() != null && 
      sb.toString().equals("/PLACEMENTRESPONSE/PLACEMENTDECISION/PLACEMENTOPPORTUNITYREF") && 
      (!((String)r[posXMLDataTypeDescription]).equalsIgnoreCase("START_ELEMENT")) ) {     
     oppoIdVal = (String) r[posXMLDataValue];
  }

  Object[] outputRowData = createOutputRow(r, data.outputRowMeta.size());
  outputRowData[posOutputGroup]=new Long(groupVal);
  outputRowData[posOutputXMLKey]=new String(sb.toString());
  outputRowData[posOutputRespMsgId]=new String(respMsgIdVal);
  outputRowData[posOutputplcmtGroup]=new Long(plcmtGroupVal);
  outputRowData[posOutputSessionId]=new String(sessionIdVal);
  outputRowData[posOutputDecisionId]=new String(decisionIdVal);
  outputRowData[posOutputOppoId]=new String(oppoIdVal);

  putRow(data.outputRowMeta, outputRowData);

  return true;
}]]></class_source>
        </definition>
