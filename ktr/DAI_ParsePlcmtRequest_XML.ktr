        <class_source><![CDATA[import com.lexonics.base64.Base64Codec;
import java.io.FileInputStream;
import java.io.IOException;
import java.io.ObjectInputStream;
import java.io.PrintStream;
import java.security.*;
import javax.crypto.*;
import javax.crypto.spec.*;

private int cryptographyKeySize=0;
private String cryptographyMethod=null;
private String keyPath=null;
private KeyGenerator kgen = null;
private int secretKeyFieldIndex=0;
private int unencryptedTextFieldIndex=0;
private int mySecretFieldIndex=0;
private SecretKey secretKey = null;
private Cipher cipher = null;


private int posInputTargetCode; // input: TARGET_CODE
private int posInputTermAddr;   // input: TERMINAL_ADDR

private int posOutputLocalDate; // local date 
private int posOutputLocalDT;   // timestamp with time zone offset
private int posOutputLocalHr;   // local hour
private int posOutputLocalOffset;  // local hour
private int posOutputUTC;          // UTC timestamp
private int posEncryptHH;          // encrypted HH
private int posEncryptSTB;         // encrypted STB

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException {
  Object[] r=getRow();

  if (r==null) {
     setOutputDone();
    return false;
  }

  if (first) {
    // get field positions (input)
     posInputTargetCode = getInputRowMeta().indexOfValue("TARGET_CODE");  
     posInputTermAddr   = getInputRowMeta().indexOfValue("TERMINAL_ADDR");

    // get field positions (output)
     posOutputLocalDate  = data.outputRowMeta.indexOfValue("local_date");
     posOutputLocalDT    = data.outputRowMeta.indexOfValue("local_dt");
     posOutputLocalHr    = data.outputRowMeta.indexOfValue("local_hr");
     posOutputLocalOffset= data.outputRowMeta.indexOfValue("local_offset");
     posOutputUTC        = data.outputRowMeta.indexOfValue("utc");
     posEncryptHH        = data.outputRowMeta.indexOfValue("enc_hh");
     posEncryptSTB       = data.outputRowMeta.indexOfValue("enc_stb");

     if (posInputTargetCode<0 || 
         posInputTermAddr<0 || 
         posOutputLocalDate<0 || 
         posOutputLocalDT<0 ||  
         posOutputLocalHr<0 ||  
         posOutputLocalOffset<0 ||  
         posOutputUTC<0 ||
         posEncryptHH<0 ||
         posEncryptSTB<0) {
         throw new KettleException("One of input/output fields was not found.");
     }

     cryptographyMethod=getVariable("cryptographyMethod", "");
     cryptographyKeySize=Const.toInt(getVariable("cryptographyKeySize", ""),0);

     // get cipher
     keyPath=System.getenv("KEY_PATH");
     try {
        ObjectInputStream ois = new ObjectInputStream(new FileInputStream(keyPath));

        try {
          secretKey = (SecretKey) ois.readObject();
        } finally {
          try { ois.close(); } catch (IOException e) { }
        }
     } catch (Exception e) {
        // IOException
        // ClassNotFoundException
        throw new KettleException("cryptographyMethod" + cryptographyMethod + " Cannot get key from keyPath. Check parameter 'keyPath'!" + keyPath);
     }

     try {
        cipher = Cipher.getInstance(cryptographyMethod);
        cipher.init(Cipher.ENCRYPT_MODE, secretKey);
     } catch (Exception ex) {
        throw new KettleStepException(ex);
     }

     first=false;
  }


  Object[] outputRowData = createOutputRow(r, data.outputRowMeta.size());

  byte[] encryptedHH = null;
  byte[] encryptedSTB = null;
  try {
	if (r[posInputTargetCode] != null) {
	    byte[] utf8HH = ((String)r[posInputTargetCode]).getBytes("UTF-8");
	    encryptedHH = cipher.doFinal(utf8HH);
  		outputRowData[posEncryptHH]  = Base64Codec.encode(encryptedHH);
	}
	if (r[posInputTermAddr] != null){
	    byte[] utf8STB = ((String)r[posInputTermAddr]).getBytes("UTF-8");
    	// Encrypt
	    encryptedSTB = cipher.doFinal(utf8STB);
	    outputRowData[posEncryptSTB] = Base64Codec.encode(encryptedSTB);
	}

  } catch (Exception e) {
    throw new KettleStepException(e);
  }

//  outputRowData[posOutputLocalDate]  = "LocalDT Val";
//  outputRowData[posOutputLocalDT]    = "";
//  outputRowData[posOutputLocalHr]    = new Long(99);
//  outputRowData[posOutputLocalOffset]= new Long(98);
//  outputRowData[posOutputUTC]        = "";

  putRow(data.outputRowMeta, outputRowData);

  return true;
