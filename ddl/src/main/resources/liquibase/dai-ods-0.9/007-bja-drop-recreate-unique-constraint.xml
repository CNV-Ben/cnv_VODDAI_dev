<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
     xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
     xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
     http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">
  <changeSet id="007_1" author="bja">
      <!-- Drop existing Unique Constraint -->
      <dropUniqueConstraint tableName="rpt_audit_agg_campaign_detail" constraintName="uq_rpt_agg_camp_dtl"/>
      <!--   -->
      <!-- Create revised Unique Constraint -->
      <addUniqueConstraint tableName="rpt_audit_agg_campaign_detail"
          columnNames="campaign_id, campaign_item_id, media_asset_id, service_group, date_hour, entertainment_provider_id"
          constraintName="uq_rpt_agg_camp_dtl"/>
  </changeSet>
</databaseChangeLog>
